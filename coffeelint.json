{
  "___NOTE__",
  "Please move enabled rules to here",
  "And use 'error' level since --quiet option will omit 'warn' level rules.",
  "Other available rules are located in 'disabled_rules.json'."
  "___NOTE END___"

  "arrow_spacing": {
    "level": "error"
  },
  "braces_spacing": {
    "level": "error",
    "spaces": 0,
    "empty_object_spaces": 0
  },
  "camel_case_classes": {
    "level": "error"
  },
  "duplicate_key": {
    "level": "error"
  },
  "indentation": {
    "value": 2,
    "level": "error"
  },
  "line_endings": {
    "level": "error",
    "value": "unix"
  },
  "no_debugger": {
    "level": "error",
    "console": false
  },
  "no_empty_functions": {
    "level": "error"
  },
  "no_empty_param_list": {
    "level": "error"
  },
  "no_plusplus": {
    "level": "error"
  },
  "no_private_function_fat_arrows": {
    "level": "error"
  },
  "no_tabs": {
    "level": "error"
  },
  "no_this": {
    "level": "error"
  },
  "no_throwing_strings": {
    "level": "error"
  },
  "no_trailing_semicolons": {
    "level": "error"
  },
  "no_trailing_whitespace": {
    "level": "error",
    "allowed_in_comments": false,
    "allowed_in_empty_lines": true
  },
  "prefer_english_operator": {
    "level": "error",
    "doubleNotLevel": "ignore",
    "ops": [
      "and",
      "or",
      "not",
      "is",
      "isnt"
    ]
  },
  "space_operators": {
    "level": "error",
    "default_parameters": true
  },
  "spacing_after_comma": {
    "level": "error",
    "ignore_elision": false
  },
  "no_implicit_returns": {
    "name": "no_implicit_returns",
    "level": "error"
  }
}
